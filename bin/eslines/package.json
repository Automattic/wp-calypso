{
  "name": "eslines",
  "version": "0.0.13",
  "description": "Utility to process ESLint JSON reports",
  "keywords": ["eslint"],
  "license": "GPL-2.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/Automattic/eslines.git"
  },
  "main": "./index.js",
  "bin": {
    "eslines": "./index.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint --fix .",
    "test": "./node_modules/.bin/tape tests/test*js | ./node_modules/.bin/faucet",
    "test-differ": "./node_modules/.bin/tape tests/test-differ.js | ./node_modules/.bin/faucet",
    "test-enforce": "./node_modules/.bin/tape tests/test-enforce.js | ./node_modules/.bin/faucet",
    "test-downgrade-unmodified-lines": "./node_modules/.bin/tape tests/test-downgrade-unmodified-lines.js | ./node_modules/.bin/faucet",
    "test-exit-codes": "./node_modules/.bin/tape tests/test-exit-codes.js | ./node_modules/.bin/faucet",
    "test-filter-parsing-errors": "./node_modules/.bin/tape tests/test-filter-parsing-errors.js | ./node_modules/.bin/faucet"
  },
  "dependencies": {
    "optionator": "0.8.1"
  },
  "devDependencies": {
    "tape": "4.6.0",
    "faucet": "0.0.1",
    "eslint": "3.8.1",
    "eslint-config-wpcalypso": "0.8.0",
    "eslint-plugin-wpcalypso": "3.4.1"
  }
}
