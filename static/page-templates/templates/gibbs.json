	"content": "<!-- wp:cover {\"url\":\"https:\\/\\/dotcompatterns.files.wordpress.com\\/2020\\/04\\/louis-hansel-shotsoflouis-d0ueoxdvj5i-unsplash.jpg\",\"id\":181,\"dimRatio\":20,\"customOverlayColor\":\"#000000\",\"focalPoint\":{\"x\":0.5,\"y\":\"0.70\"},\"minHeight\":620,\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-20 has-background-dim\" style=\"background-image:url('https://dotcompatterns.files.wordpress.com/2020/04/louis-hansel-shotsoflouis-d0ueoxdvj5i-unsplash.jpg');background-color:#000000;background-position:50% 70%;min-height:620px;\"><div class=\"wp-block-cover__inner-container\"><!-- wp:jetpack/layout-grid {\"addGutterEnds\":false,\"column1DesktopSpan\":10,\"column1DesktopOffset\":1,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"column2DesktopOffset\":1,\"className\":\"column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1 wp-block-jetpack-layout-gutter__nowrap\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:paragraph {\"align\":\"center\",\"style\":{\"color\":{\"text\":\"#ffffff\"},\"typography\":{\"fontSize\":80,\"lineHeight\":\"1.1\"}}} -->\n<p class=\"has-text-align-center has-text-color\" style=\"line-height:1.1;font-size:80px;color:#ffffff;\"><strong>Get it delivered</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"style\":{\"color\":{\"text\":\"#ffffff\"}}} -->\n<p class=\"has-text-align-center has-text-color\" style=\"color:#ffffff;\">If you can’t come to us, we’ll go to you.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:buttons {\"align\":\"center\"} -->\n<div class=\"wp-block-buttons aligncenter\"><!-- wp:button {\"style\":{\"color\":{\"text\":\"#ffffff\",\"background\":\"#a5150f\"}}} -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link has-text-color has-background\" style=\"background-color:#a5150f;color:#ffffff;\">Order now</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:buttons --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:spacer -->\n<div style=\"height:100px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"column1DesktopSpan\":10,\"column1DesktopOffset\":1,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"column2DesktopOffset\":2,\"className\":\"column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:paragraph {\"className\":\"margin-bottom-half\",\"style\":{\"typography\":{\"fontSize\":49,\"lineHeight\":\"1.2\"}}} -->\n<p class=\"margin-bottom-half\" style=\"line-height:1.2;font-size:49px;\"><strong>Burgers</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"className\":\"margin-top-half\",\"style\":{\"typography\":{\"lineHeight\":\"1.5\",\"fontSize\":16}}} -->\n<p class=\"margin-top-half\" style=\"line-height:1.5;font-size:16px;\">All burgers come with rosemary salted chips.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Awesome Burger</strong><br>The burger that made us famous. 100% pure lean beef grilled to perfection.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Chicken Sandwich</strong><br>Cajun chicken breast with lettuce and tomato on a freshly toasted bun.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Steak Burger</strong><br>The burger made from 100% pure, succulent flame-grilled Black Angus beef.<br><strong>$8.50</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Bacon Burger</strong><br>Flame-grilled 100% British and Irish beef burgers, topped a with thick-cut smoked bacon.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Veggie Burger</strong><br>A delicious, soy, boca patty, served on a freshly toasted, whole-wheat bun.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Kids Burger</strong><br>Made from 100% beef topped with a slice of melted cheese and ketchup, all wrapped up in a sesame seed bun.<br><strong>$6.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:spacer -->\n<div style=\"height:100px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"column1DesktopSpan\":10,\"column1DesktopOffset\":1,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"column2DesktopOffset\":2,\"className\":\"column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":49,\"lineHeight\":\"1.2\"}}} -->\n<p style=\"line-height:1.2;font-size:49px;\"><strong>Salads</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Classic Cobb</strong><br>Mixed lettuce leaves, blue cheese, sharp cheddar, fried egg, tomatoes &amp; onion with buttermilk ranch dressing.<br><strong>$6.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Spinach, Cucumber &amp; Goats Cheese</strong><br>Spinach, goats cheese, raisins, dried cranberries, cucumbers, tomatoes &amp; onions with balsamic dressing.<br><strong>$6.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Baja Cobb</strong><br>Mixed lettuce leaves, sharp cheddar, applewood smoked bacon, guacamole, tomatoes &amp; red onion with chipotle dressing.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Harvest</strong><br>Mixed lettuce leaves, blue cheese, raisins, dried cranberries, sunflower seeds, pumpkin seeds &amp; tomatoes with balsamic dressing.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:spacer -->\n<div style=\"height:100px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"column1DesktopSpan\":10,\"column1DesktopOffset\":1,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"column2DesktopOffset\":2,\"className\":\"column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":49,\"lineHeight\":\"1.2\"}}} -->\n<p style=\"line-height:1.2;font-size:49px;\"><strong>Desserts</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Ice Cream in Mini Pots</strong><br>Vanilla / Strawberries &amp; Cream / Belgian Chocolate<br><strong>$4.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Churros</strong><br>Served with salted caramel &amp; Chocolate.<br><strong>$4.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Ice Cream Sandwich</strong><br>Vanilla ice cream sandwiched in a sugar coated, jam filled doughnut.<br><strong>$5.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph -->\n<p><strong>Cheesecake Shake</strong><br>Vanilla &amp; chocolate milkshake, malt, chocolate sauce, vanilla whipped cream.<br><strong>$8.00</strong></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:spacer -->\n<div style=\"height:100px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"color\":{\"text\":\"#ffffff\",\"background\":\"#242424\"}}} -->\n<div class=\"wp-block-group alignfull has-text-color has-background\" style=\"background-color:#242424;color:#ffffff;\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":60} -->\n<div style=\"height:60px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"addGutterEnds\":false,\"column1DesktopSpan\":5,\"column1DesktopOffset\":1,\"column1TabletSpan\":4,\"column1MobileSpan\":2,\"column2DesktopSpan\":5,\"column2TabletSpan\":4,\"column2MobileSpan\":2,\"column2MobileOffset\":2,\"column3DesktopOffset\":1,\"column3MobileOffset\":6,\"className\":\"column1-desktop-grid__span-5 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column2-desktop-grid__span-5 column2-desktop-grid__start-7 column2-desktop-grid__row-1 column1-tablet-grid__sp