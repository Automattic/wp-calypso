	"content": "<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"background-light\"} -->\n<div class=\"wp-block-group alignfull has-background-light-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"level\":1,\"textColor\":\"secondary\"} -->\n<h1 class=\"has-secondary-color has-text-color\">Excel</h1>\n<!-- /wp:heading -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column {\"width\":80} -->\n<div class=\"wp-block-column\" style=\"flex-basis:80%;\"><!-- wp:paragraph {\"textColor\":\"foreground-dark\"} -->\n<p class=\"has-foreground-dark-color has-text-color\">Stratford is a place of teaching excellence, where your children can feel at home while learning the skills that will help them thrive after their school years are over.  As a parent, what more can you ask for?</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":20} -->\n<div class=\"wp-block-column\" style=\"flex-basis:20%;\"><!-- wp:button {\"backgroundColor\":\"secondary\",\"textColor\":\"background\",\"className\":\"is-style-default\"} -->\n<div class=\"wp-block-button is-style-default\"><a class=\"wp-block-button__link has-background-color has-secondary-background-color has-text-color has-background\" href=\"https://stratforddemo.wordpress.com/contact/\">get in touch</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/stratforddemo.files.wordpress.com\\/2019\\/08\\/children-close-up-crowd-764681-1.jpg\",\"id\":226,\"dimRatio\":0,\"focalPoint\":{\"x\":0.5,\"y\":0.01},\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull\" style=\"background-image:url('https://stratforddemo.files.wordpress.com/2019/08/children-close-up-crowd-764681-1.jpg');background-position:50% 1%;\"><div class=\"wp-block-cover__inner-container\"><!-- wp:paragraph {\"align\":\"center\",\"placeholder\":\"Write titleu2026\",\"fontSize\":\"large\"} -->\n<p class=\"has-text-align-center has-large-font-size\"></p>\n<!-- /wp:paragraph --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"secondary\"} -->\n<div class=\"wp-block-group alignfull has-secondary-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"left\",\"textColor\":\"background\"} -->\n<h2 class=\"has-text-align-left has-background-color has-text-color\">Services</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"className\":\"has-2-columns\"} -->\n<div class=\"wp-block-columns has-2-columns\"><!-- wp:column {\"width\":20} -->\n<div class=\"wp-block-column\" style=\"flex-basis:20%;\"><!-- wp:image {\"id\":115,\"width\":150,\"height\":150,\"sizeSlug\":\"large\"} -->\n<figure class=\"wp-block-image size-large is-resized\"><img src=\"https://stratforddemo.files.wordpress.com/2019/08/icon-implementation-support-1.png\" alt=\"\" class=\"wp-image-115\" width=\"150\" height=\"150\" /></figure>\n<!-- /wp:image --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":80} -->\n<div class=\"wp-block-column\" style=\"flex-basis:80%;\"><!-- wp:heading {\"level\":3,\"className\":\"margin-bottom-half\"} -->\n<h3 class=\"margin-bottom-half\">Individual Attention</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"margin-top-half\",\"textColor\":\"background\",\"fontSize\":\"small\"} -->\n<p class=\"margin-top-half has-background-color has-text-color has-small-font-size\">The classroom environment at Stratford allows your childs' educators to give them the time and attention that they need in order to succeed. We believe in keeping class numbers low to maximum learning potential.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"className\":\"has-2-columns\"} -->\n<div class=\"wp-block-columns has-2-columns\"><!-- wp:column {\"width\":20} -->\n<div class=\"wp-block-column\" style=\"flex-basis:20%;\"><!-- wp:image {\"id\":114,\"width\":150,\"height\":150,\"sizeSlug\":\"large\"} -->\n<figure class=\"wp-block-image size-large is-resized\"><img src=\"https://stratforddemo.files.wordpress.com/2019/08/icon-managed-hosting-1.png\" alt=\"\" class=\"wp-image-114\" width=\"150\" height=\"150\" /></figure>\n<!-- /wp:image --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":80} -->\n<div class=\"wp-block-column\" style=\"flex-basis:80%;\"><!-- wp:heading {\"level\":3,\"className\":\"margin-bottom-half\"} -->\n<h3 class=\"margin-bottom-half\">Fantastic Facilities</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"margin-top-half\",\"textColor\":\"background\",\"fontSize\":\"small\"} -->\n<p class=\"margin-top-half has-background-color has-text-color has-small-font-size\">Each student has access to the best possible learning technologies, as well having guest classes from industry professionals. We believe this better prepares your child for their careers or own businesses.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"className\":\"has-2-columns\"} -->\n<div class=\"wp-block-columns has-2-columns\"><!-- wp:column {\"width\":20} -->\n<div class=\"wp-block-column\" style=\"flex-basis:20%;\"><!-- wp:image {\"id\":116,\"width\":150,\"height\":150,\"sizeSlug\":\"large\"} -->\n<figure class=\"wp-block-image size-large is-resized\"><img src=\"https://stratforddemo.files.wordpress.com/2019/08/icon-build-digital-experiences-1.png\" alt=\"\" class=\"wp-image-116\" width=\"150\" height=\"150\" /></figure>\n<!-- /wp:image --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":80} -->\n<div class=\"wp-block-column\" style=\"flex-basis:80%;\"><!-- wp:heading {\"level\":3,\"className\":\"margin-bottom-half\"} -->\n<h3 class=\"margin-bottom-half\">Life Long Learning</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"margin-top-half\",\"textColor\":\"background\",\"fontSize\":\"small\"} -->\n<p class=\"margin-top-half has-background-color has-text-color has-small-font-size\">We believe in equipping your children with the skills to be able to learn for the rest of their lives! By instilling in them a method of analytical thinking, we believe they will be able to be independent thinkers and be high performers in their chosen careers.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:group {\"align\":\"full\"} -->\n<div class=\"wp-block-group alignfull\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"left\"} -->\n<h2 class=\"has-text-align-left\">What other parents say</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"className\":\"has-2-columns\"} -->\n<div class=\"wp-block-columns has-2-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:quote -->\n<blockquote class=\"wp-block-quote\"><p><em>My son struggled with mathematics during his school years - until we enrolled him at Stratford. That has been the best decision we made, and his educators have worked hard to get his mathematics to a level we never thought possible!</em></p><cite>― Anthony, Grade 8 parent</cite></blockquote>\n<!-- /wp:quote --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:quote {\"className\":\"is-style-default\"} -->\n<blockquote class=\"wp-block-quote is-style-default\"><p><em>My daughter has loved being at Stratford for the last 5 yeras. The learning environment makes the difference and I'm so proud that she has been accepted into University next year!</em></p><cite>― Megan, Grade 12 parent</cite></blockquote>\n<!-- /wp:quote --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/stratforddemo.files.wordpress.com\\/2019\\/08\\/auditorium-benches-chairs-207691-1-1.jpg\",\"id\":228,\"dimRatio\":80,\"overlayColor\":\"foreground-dark\",\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-80 has-foreground-dark-background-color has-background-dim\" style=\"background-image:url('https://stratforddemo.files.wordpress.com/2019/08/auditorium-benches-chairs-207691-1-1.jpg');\"><div class=\"wp-block-cover__inner-container\"><!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"className\":\"has-3-columns\"} -->\n<div class=\"wp-block-columns has-3-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>About Us</h4>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>We are Stratford, a place of educational excellence.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>We equip your children for their chosen field of study and careers with lifelong learning skills.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>News</h4>\n<!-- /wp:heading -->\n\n<!-- wp:latest-posts {\"postsToShow\":3,\"displayPostDate\":true} /--></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>Address</h4>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>mail@example.com<br>1-800-123-456<br>99 Education Way<br>Stratford, London<br>GXVM+89<br>United Kingdom</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:cover -->"
}
