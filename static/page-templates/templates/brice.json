 /wp:buttons -->\n\n<!-- wp:spacer {\"height\":30} -->\n<div style=\"height:30px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group --></div></div>\n<!-- /wp:cover --></div>\n<!-- /wp:jetpack/layout-grid-column -->\n\n<!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:cover {\"customOverlayColor\":\"#24135f\"} -->\n<div class=\"wp-block-cover has-background-dim\" style=\"background-color:#24135f;\"><div class=\"wp-block-cover__inner-container\"><!-- wp:group {\"style\":{\"color\":{\"background\":\"#24135f\"}}} -->\n<div class=\"wp-block-group has-background\" style=\"background-color:#24135f;\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":30} -->\n<div style=\"height:30px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#fcdc3e\"},\"typography\":{\"fontSize\":41}}} -->\n<p class=\"has-text-color\" style=\"font-size:41px;color:#fcdc3e;\"><strong>Are you a business?</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"style\":{\"color\":{\"text\":\"#fcdc3e\"}}} -->\n<p class=\"has-text-color\" style=\"color:#fcdc3e;\">We are uniting our resources around this challenge, and we are combining our resources and asks to make it easy for people to support their communities.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:buttons -->\n<div class=\"wp-block-buttons\"><!-- wp:button {\"style\":{\"color\":{\"text\":\"#24135f\",\"background\":\"#fcdc3e\"}},\"className\":\"is-style-outline\"} -->\n<div class=\"wp-block-button is-style-outline\"><a class=\"wp-block-button__link has-text-color has-background\" style=\"background-color:#fcdc3e;color:#24135f;\">Get involved</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:buttons -->\n\n<!-- wp:spacer {\"height\":30} -->\n<div style=\"height:30px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group --></div></div>\n<!-- /wp:cover --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"background\"} -->\n<div class=\"wp-block-group alignfull has-background-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"gutterSize\":\"none\",\"addGutterEnds\":false,\"column1DesktopSpan\":10,\"column1DesktopOffset\":1,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"column2DesktopSpan\":8,\"column2DesktopOffset\":23,\"column2TabletSpan\":4,\"column2MobileSpan\":4,\"column3DesktopOffset\":8,\"className\":\"column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-10 column1-desktop-grid__start-2 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1 wp-block-jetpack-layout-gutter__nowrap wp-block-jetpack-layout-gutter__none\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":41}}} -->\n<p style=\"font-size:41px;\"><strong>Hear more from our community</strong>.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"gutterSize\":\"none\",\"ad