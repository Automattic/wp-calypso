	"__file": "wp_block",
	"title": "rockfield",
	"content": "<!-- wp:cover {\"url\":\"https://rockfielddemo.files.wordpress.com/2019/08/appetizers-chef-cuisine-1267320-1-1.jpg\",\"id\":217,\"hasParallax\":true,\"dimRatio\":20,\"customOverlayColor\":\"#000000\",\"align\":\"full\",\"className\":\"a8c-image-background\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-20 has-background-dim has-parallax a8c-image-background\" style=\"background-image:url(https://rockfielddemo.files.wordpress.com/2019/08/appetizers-chef-cuisine-1267320-1-1.jpg);background-color:#000000\"><div class=\"wp-block-cover__inner-container\"><!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"className\":\"a8c-text-title\"} -->\n<h2 class=\"has-text-align-center a8c-text-title\">Enjoy Our Food Experience</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"level\":4,\"className\":\"a8c-text-title\"} -->\n<h4 class=\"has-text-align-center a8c-text-title\">Welcome to Rockfield</h4>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:group -->\n<div class=\"wp-block-group\"><div class=\"wp-block-group__inner-container\"><!-- wp:paragraph {\"className\":\"a8c-text-description\"} -->\n<p class=\"a8c-text-description\">Chef Patron Joan Smith and Executive Chef François Lemoine have teamed up to open one of the most exciting restaurants in Anytown.<br><br>Our specialty is delicious&nbsp;cuisine created with the ingenuity and passion inspired by the fantastic flavors of France&nbsp;itself. We take great pride in improving upon your favorite French&nbsp;dishes in new and creative ways.</p>\n<!-- /wp:paragraph --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:cover {\"url\":\"https://rockfielddemo.files.wordpress.com/2019/08/blur-chef-close-up-671956-1.jpg\",\"id\":218,\"hasParallax\":true,\"dimRatio\":20,\"customOverlayColor\":\"#000000\",\"align\":\"full\",\"className\":\"a8c-image-background\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-20 has-background-dim has-parallax a8c-image-background\" style=\"background-image:url(https://rockfielddemo.files.wordpress.com/2019/08/blur-chef-close-up-671956-1.jpg);background-color:#000000\"><div class=\"wp-block-cover__inner-container\"><!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"className\":\"a8c-text-title\"} -->\n<h2 class=\"has-text-align-center a8c-text-title\">Discover Our Menu</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"className\":\"a8c-text-description\"} -->\n<p class=\"a8c-text-description\">The menu is a mix of French&nbsp;regional cuisines, and the menu changes with the seasons.<br>Be sure to ask about the daily specials!</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"level\":4,\"className\":\"a8c-text-title\"} -->\n<h4 class=\"has-text-align-center a8c-text-title\">Sample Menu</h4>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":6,\"className\":\"a8c-text-examples-title\"} -->\n<h6 class=\"a8c-text-examples-title\">Starter</h6>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"a8c-text-examples-description\"} -->\n<p class=\"a8c-text-examples-description\"><strong>Fricassée of Wild Mushrooms</strong><br><em>Braised wild mushrooms served with seaweed and liquid nitrogen</em><br>$14.50</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":6,\"className\":\"a8c-text-examples-title\"} -->\n<h6 class=\"a8c-text-examples-title\">Main</h6>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"a8c-text-examples-description\"} -->\n<p class=\"a8c-text-examples-description\"><strong>Coq au Vin d'Alsace<br></strong><em>Chicken cooked in Riesling with onions, mushrooms and herbs</em><br>$25.50<br></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":6,\"className\":\"a8c-text-examples-title\"} -->\n<h6 class=\"a8c-text-examples-title\">Dessert</h6>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"className\":\"a8c-text-examples-description\"} -->\n<p class=\"a8c-text-examples-description\"><strong>Pumpkin Soufflé</strong><br><em>With bourbon, cinnamon and molasses sauce</em><br>$9.00</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:button {\"align\":\"center\",\"className\":\"a8c-button\"} -->\n<div class=\"wp-block-button aligncenter a8c-button\"><a class=\"wp-block-button__link\" href=\"https://rockfielddemo.wordpress.com/menu/\">View Full Menu</a></div>\n<!-- /wp:button -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:cover {\"url\":\"https://rockfielddemo.files.wordpress.com/2019/08/ceiling-chairs-clean-2225570-1-1.jpg\",\"id\":219,\"hasParallax\":true,\"dimRatio\":20,\"customOverlayColor\":\"#000000\",\"align\":\"full\",\"className\":\"a8c-image-background\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-20 has-background-dim has-parallax a8c-image-background\" style=\"background-image:url(https://rockfielddemo.files.wordpress.com/2019/08/ceiling-chairs-clean-2225570-1-1.jpg);background-color:#000000\"><div class=\"wp-block-cover__inner-container\"><!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"className\":\"a8c-text-title\"} -->\n<h2 class=\"has-text-align-center a8c-text-title\">Book Your Table</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":20} -->\n<div style=\"height:20px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator -->\n<hr class=\"wp-block-separator\"/>\n<!-- /wp:separator --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"className\":\"a8c-text-contact\"} -->\n<p class=\"a8c-text-contact\">To book a table, call us (123) 444–5678 or use the form below.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:jetpack/contact-form -->\n<!-- wp:jetpack/field-name {\"required\":true} /-->\n<!-- wp:jetpack/field-email {\"required\":true} /-->\n<!-- wp:jetpack/field-telephone /-->\n<!-- wp:jetpack/field-date {\"label\":\"Date\",\"required\":true} /-->\n<!-- wp:jetpack/field-select {\"label\":\"Time\",\"required\":true,\"options\":[\"11:30 am\",\"12:00 pm\",\"12:30 pm\",\"1:00 pm\",\"1:30 pm\",\"2:00 pm\",\"7:00 pm\",\"7:30 pm\",\"8:00 pm\",\"8:30 pm\",\"9:00 pm\",\"9:30 pm\"]} /-->\n<!-- wp:jetpack/field-select {\"label\":\"Party size\",\"required\":true,\"options\":[\"1 person\",\"2 people\",\"3 people\",\"4 people\",\"5 people\",\"6 people\",\"Larger party\"]} /-->\n<!-- wp:jetpack/field-textarea {\"label\":\"Special requests\"} /-->\n<!-- /wp:jetpack/contact-form -->\n\n<!-- wp:spacer {\"height\":84} -->\n<div style=\"height:84px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:cover {\"url\":\"https://rockfielddemo.files.wordpress.com/2019/08/boston-https-whitlockdesigns-com-interior-design-2225579-1-1.jpg\",\"id\":220,\"hasParallax\":true,\"dimRatio\":60,\"customOverlayColor\":\"#000000\",\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-60 has-background-dim has-parallax\" style=\"background-image:url(https://rockfielddemo.files.wordpress.com/2019/08/boston-https-whitlockdesigns-com-interior-design-2225579-1-1.jpg);background-color:#000000\"><div class=\"wp-block-cover__inner-container\"><!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"huge\",\"className\":\"a8c-text-title\"} -->\n<p class=\"has-text-align-center has-huge-font-size a8c-text-title\"><strong>Rockfield</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"className\":\"a8c-text-description\"} -->\n<p class=\"has-text-align-center a8c-text-description\">Visitors will want to know who is on the other side of the page. Use this space to write about your business.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32,\"className\":\"desktop-only\"} -->\n<div style=\"height:32px\" aria-hidden=\"true\" class=\"wp-block-spacer desktop-only\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":5,\"className\":\"a8c-text-example-title\"} -->\n<h5 class=\"a8c-text-example-title\">Open Hours</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"fontSize\":\"small\",\"className\":\"a8c-text-example-description\"} -->\n<p class=\"has-text-align-left has-small-font-size a8c-text-example-description\"><strong>Mon:</strong> closed<br><strong>Tue - Sun:</strong> 11:30 am - 10:00 pm</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"align\":\"left\",\"level\":5,\"className\":\"a8c-text-example-title\"} -->\n<h5 class=\"has-text-align-left a8c-text-example-title\">Location</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"fontSize\":\"small\",\"className\":\"a8c-text-example-description\"} -->\n<p class=\"has-text-align-left has-small-font-size a8c-text-example-description\">1 Example Street,<br>Anytown, NY 10100</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"align\":\"left\",\"level\":5,\"className\":\"a8c-text-example-title\"} -->\n<h5 class=\"has-text-align-left a8c-text-example-title\">Reservation</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"fontSize\":\"small\",\"className\":\"a8c-text-example-description\"} -->\n<p class=\"has-text-align-left has-small-font-size a8c-text-example-description\">(555) 555 1234<br>mail@example.com</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns --></div></div>\n<!-- /wp:cover -->"
}
