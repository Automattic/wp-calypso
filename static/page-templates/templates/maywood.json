	"content": "<!-- wp:cover {\"url\":\"https:\\/\\/maywooddemo.files.wordpress.com\\/2019\\/08\\/louis-hansel-nkmc3kluihu-unsplash-1.jpg\",\"id\":100,\"dimRatio\":30,\"overlayColor\":\"foreground-dark\",\"focalPoint\":{\"x\":0.5,\"y\":0.5},\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-30 has-foreground-dark-background-color has-background-dim\" style=\"background-image:url('https://maywooddemo.files.wordpress.com/2019/08/louis-hansel-nkmc3kluihu-unsplash-1.jpg');background-position:50% 50%;\"><div class=\"wp-block-cover__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":1} -->\n<h1>Enjoy Our Food Experience</h1>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>We don't just make food. We make people's experience. Maywood was built on the belief that food should be special, and we carry that belief into everything we do.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:button {\"borderRadius\":0,\"align\":\"none\"} -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link no-border-radius\" href=\"/menu\">View our menu</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"background\"} -->\n<div class=\"wp-block-group alignfull has-background-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:paragraph {\"className\":\"margin-bottom-half\",\"fontSize\":\"small\"} -->\n<p class=\"margin-bottom-half has-small-font-size\"><span class=\"uppercase\">OUR STOR</span><span class=\"uppercase\">Y</span></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:heading {\"align\":\"left\",\"className\":\"margin-top-half\"} -->\n<h2 class=\"has-text-align-left margin-top-half\">Welcome to Maywood</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"left\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-align-left has-small-font-size\">Chef Patron Joan Smith and Executive Chef François Lemoine have teamed up to open one of the most exciting restaurants in Anytown.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">Our speciality is delicious&nbsp;cuisine created with the ingenuity and passion inspired by the fantastic flavours of France&nbsp;itself. We take great pride in improving upon your favourite French&nbsp;dishes in new and creative ways.</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:image {\"id\":64,\"sizeSlug\":\"large\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://maywooddemo.files.wordpress.com/2019/08/tatiana-zhukova-mejzeyi0pji-unsplash.jpg?w=768\" alt=\"\" class=\"wp-image-64\" /></figure>\n<!-- /wp:image --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:image {\"id\":59,\"sizeSlug\":\"large\"} -->\n<figure class=\"wp-block-image size-large\"><img src=\"https://maywooddemo.files.wordpress.com/2019/08/chef-cook-cooked-1327215.jpg?w=763\" alt=\"\" class=\"wp-image-59\" /></figure>\n<!-- /wp:image -->\n\n<!-- wp:spacer {\"height\":64} -->\n<div style=\"height:64px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/maywooddemo.files.wordpress.com\\/2019\\/08\\/louis-hansel-in24hltoqyi-unsplash.jpg\",\"id\":99,\"dimRatio\":30,\"overlayColor\":\"foreground-dark\",\"focalPoint\":{\"x\":0.5,\"y\":0.5},\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-30 has-foreground-dark-background-color has-background-dim\" style=\"background-image:url('https://maywooddemo.files.wordpress.com/2019/08/louis-hansel-in24hltoqyi-unsplash.jpg');background-position:50% 50%;\"><div class=\"wp-block-cover__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\"} -->\n<h2 class=\"has-text-align-center\">Discover Our Menu</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\">The menu is a mix of French regional cuisines, and the menu changes with the seasons. Be sure to ask about the daily specials!</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:button {\"borderRadius\":0,\"align\":\"center\"} -->\n<div class=\"wp-block-button aligncenter\"><a class=\"wp-block-button__link no-border-radius\" href=\"/menu\">View Our Menu</a></div>\n<!-- /wp:button -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"background\"} -->\n<div class=\"wp-block-group alignfull has-background-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\",\"level\":3} -->\n<h3 class=\"has-text-align-center\">Sample Menu</h3>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>Starter</h4>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p><strong>Fricassée of Wild Mushrooms</strong><br><em>Braised wild mushrooms served with seaweed and liquid nitrogen</em><br>$14.50</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>Main</h4>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p><strong>Coq au Vin d'Alsace<br></strong><em>Chicken cooked in Riesling with onions, mushrooms and herbs</em><br>$25.50</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:heading {\"level\":4} -->\n<h4>Dessert</h4>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p><strong>Pumpkin Soufflé</strong><br><em>With bourbon, cinnamon and molasses sauce</em><br>$9.00</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:button {\"borderRadius\":0,\"align\":\"center\"} -->\n<div class=\"wp-block-button aligncenter\"><a class=\"wp-block-button__link no-border-radius\" href=\"/menu\">View Our Full Menu</a></div>\n<!-- /wp:button -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/maywooddemo.files.wordpress.com\\/2019\\/08\\/jason-leung-poi7delfiva-unsplash.jpg\",\"id\":91,\"dimRatio\":40,\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-40 has-background-dim\" style=\"background-image:url('https://maywooddemo.files.wordpress.com/2019/08/jason-leung-poi7delfiva-unsplash.jpg');\"><div class=\"wp-block-cover__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"align\":\"center\"} -->\n<h2 class=\"has-text-align-center\">Book Your Table</h2>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\">To book a table, call us (123) 444–5678 or use the form below.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:group {\"align\":\"full\",\"backgroundColor\":\"background\"} -->\n<div class=\"wp-block-group alignfull has-background-background-color has-background\"><div class=\"wp-block-group__inner-container\"><!-- wp:spacer {\"height\":48} -->\n<div style=\"height:48px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns -->\n<div class=\"wp-block-columns\"><!-- wp:column {\"width\":10} -->\n<div class=\"wp-block-column\" style=\"flex-basis:10%;\"></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":80} -->\n<div class=\"wp-block-column\" style=\"flex-basis:80%;\"><!-- wp:jetpack/contact-form -->\n<!-- wp:jetpack/field-name {\"required\":true} /-->\n\n<!-- wp:jetpack/field-email {\"required\":true} /-->\n\n<!-- wp:jetpack/field-telephone /-->\n\n<!-- wp:jetpack/field-date {\"label\":\"Date\",\"required\":true} /-->\n\n<!-- wp:jetpack/field-select {\"label\":\"Time\",\"required\":true,\"options\":[\"11:30 am\",\"12:00 pm\",\"12:30 pm\",\"1:00 pm\",\"1:30 pm\",\"2:00 pm\",\"7:00 pm\",\"7:30 pm\",\"8:00 pm\",\"8:30 pm\",\"9:00 pm\",\"9:30 pm\"]} /-->\n\n<!-- wp:jetpack/field-select {\"label\":\"Party size\",\"required\":true,\"options\":[\"1 person\",\"2 people\",\"3 people\",\"4 people\",\"5 people\",\"6 people\",\"Larger party\"]} /-->\n\n<!-- wp:jetpack/field-textarea {\"label\":\"Special requests\"} /-->\n<!-- /wp:jetpack/contact-form --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":10} -->\n<div class=\"wp-block-column\" style=\"flex-basis:10%;\"></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":48} -->\n<div style=\"height:48px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:group -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/maywooddemo.files.wordpress.com\\/2019\\/08\\/louis-hansel-3tloviwzmm4-unsplash.jpg\",\"id\":60,\"dimRatio\":80,\"overlayColor\":\"foreground-dark\",\"align\":\"full\"} -->\n<div class=\"wp-block-cover alignfull has-background-dim-80 has-foreground-dark-background-color has-background-dim\" style=\"background-image:url('https://maywooddemo.files.wordpress.com/2019/08/louis-hansel-3tloviwzmm4-unsplash.jpg');\"><div class=\"wp-block-cover__inner-container\"><!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"large\"} -->\n<p class=\"has-text-align-center has-large-font-size\"><strong><span class=\"uppercase\">Maywood</span></strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\">Visitors will want to know who is on the other side of the page. Use this space to write about your business.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\"><a href=\"http://facebook.com\">Facebook</a> / <a href=\"http://instgram.com\">Instagram</a> / <a href=\"http://foursquare.com\">Foursquare</a> / <a href=\"http://yelp.com\">Yelp</a></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:columns {\"align\":\"wide\"} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":33.5} -->\n<div class=\"wp-block-column\" style=\"flex-basis:33.5%;\"><!-- wp:heading {\"level\":5} -->\n<h5>Open Hours</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p><strong>Mon</strong> - <strong>Sat:</strong> 10:00 am – 8:00 pm<br><strong>Sun</strong>: 12:00 pm – 6:00 pm</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":33.5} -->\n<div class=\"wp-block-column\" style=\"flex-basis:33.5%;\"><!-- wp:heading {\"level\":5} -->\n<h5>Location</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>1 Example Street<br>Anytown, NY 10100 USA</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"width\":33} -->\n<div class=\"wp-block-column\" style=\"flex-basis:33%;\"><!-- wp:heading {\"level\":5} -->\n<h5>Reservation</h5>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>01234567890<br>myemail@example.com</p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n<!-- wp:spacer {\"height\":96} -->\n<div style=\"height:96px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div></div>\n<!-- /wp:cover -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph -->"
}
