:heading --></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid --></div></div>\n<!-- /wp:media-text -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading -->\n<h2>From the blog</h2>\n<!-- /wp:heading -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:jetpack/layout-grid {\"column1DesktopSpan\":12,\"column1TabletSpan\":8,\"column1MobileSpan\":4,\"className\":\"column1-desktop-grid__span-12 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"} -->\n<div class=\"wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-12 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1\"><!-- wp:jetpack/layout-grid-column -->\n<div class=\"wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none\"><!-- wp:a8c/blog-posts {\"showExcerpt\":false,\"imageShape\":\"square\",\"showAuthor\":false,\"showAvatar\":false,\"postLayout\":\"grid\",\"align\":\"full\"} /--></div>\n<!-- /wp:jetpack/layout-grid-column --></div>\n<!-- /wp:jetpack/layout-grid -->\n\n<!-- wp:cover {\"url\":\"https:\\/\\/bowenstarterdesign.files.wordpress.com\\/2020\\/07\\/bench-accounting-nvzvopqw0gc-unsplash.jpg\",\"id\":45,\"hasParallax\":true,\"overlayColor\":\"secondary\",\"align\":\"full\",\"className\":\"a8c-background\"} -->\n<div class=\"wp-block-cover alignfull has-secondary-background-color has-background-dim has-parallax a8c-background\" style=\"background-image:url('https://bowenstarterdesign.files.wordpress.com/2020/07/bench-accounting-nvzvopqw0gc-unsplash.jpg');\"><div class=\"wp-block-cover__inner-container\"><!-- wp:heading {\"className\":\"a8c-text-title\"} -