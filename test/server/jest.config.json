		"<rootDir>/server/",
		"<rootDir>/client/",
		"<rootDir>/client/extensions/"
	],
	"rootDir": "./../../",
	"testEnvironment": "node",
	"testMatch": [ "**/server/**/test/*.js?(x)" ],
