{
	"mocha_tests": [ "specs/" ],
	"mocha_opts": "test/mocha.opts",
	"mocha_args": "-R spec-xunit-reporter",
	"max_test_attempts": 3,
	"max_workers": 1,
	"suiteTag": "safaricanary",
	"sauce": false,
	"framework": "testarmada-magellan-mocha-plugin",
	"reporters": [ "./lib/reporter/magellan-reporter.js" ],
	"executors": [ "testarmada-magellan-local-executor" ],
	"bail_time": 300000
}
