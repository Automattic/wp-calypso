{
	"name": "@automattic/full-site-editing",
	"version": "1.0.0",
	"description": "Plugin for full site editing with the block editor.",
	"sideEffects": true,
	"repository": {
		"type": "git",
		"url": "git://github.com/Automattic/wp-calypso.git",
		"directory": "apps/full-site-editing"
	},
	"private": true,
	"author": "Automattic Inc.",
	"license": "GPL-2.0-or-later",
	"bugs": {
		"url": "https://github.com/Automattic/wp-calypso/issues"
	},
	"homepage": "https://github.com/Automattic/wp-calypso",
	"scripts": {
		"full-site-editing": "check-npm-client && calypso-build --source='dotcom-fse'",
		"dev:full-site-editing": "check-npm-client && npm run full-site-editing",
		"build:full-site-editing": "check-npm-client && NODE_ENV=production npm run full-site-editing",
		"posts-list-block": "check-npm-client && calypso-build --source='posts-list-block'",
		"dev:posts-list-block": "check-npm-client && npm run posts-list-block",
		"build:posts-list-block": "check-npm-client && NODE_ENV=production npm run posts-list-block",
		"common": "check-npm-client && calypso-build --source='common'",
		"dev:common": "check-npm-client && npm run common",
		"build:common": "check-npm-client && NODE_ENV=production npm run common",
		"blog-posts-block-editor": "check-npm-client && calypso-build --source='blog-posts-block/editor'",
		"blog-posts-block-view": "check-npm-client && calypso-build --source='blog-posts-block/view'",
		"blog-posts-block": "check-npm-client && npm-run-all --parallel blog-posts-block-*",
		"dev:blog-posts-block": "check-npm-client && npm run blog-posts-block",
		"build:blog-posts-block": "check-npm-client && NODE_ENV=production npm run blog-posts-block",
		"starter-page-templates": "check-npm-client && calypso-build --source='starter-page-templates'",
		"dev:starter-page-templates": "check-npm-client && npm run starter-page-templates",
		"build:starter-page-templates": "check-npm-client && NODE_ENV=production npm run starter-page-templates",
		"event-countdown-block": "check-npm-client && calypso-build --source='event-countdown-block'",
		"dev:event-countdown-block": "check-npm-client && npm run event-countdown-block",
		"build:event-countdown-block": "check-npm-client && NODE_ENV=production npm run event-countdown-block",
		"global-styles": "check-npm-client && calypso-build --source='global-styles'",
		"build:global-styles": "check-npm-client && NODE_ENV=production npm run global-styles",
		"dev:global-styles": "check-npm-client && npm run global-styles",
		"jetpack-timeline": "check-npm-client && calypso-build --source='jetpack-timeline'",
		"dev:jetpack-timeline": "check-npm-client && npm run jetpack-timeline",
		"build:jetpack-timeline": "check-npm-client && NODE_ENV=production npm run jetpack-timeline",
		"wpcom-block-editor-nux": "check-npm-client && calypso-build --source='wpcom-block-editor-nux'",
		"dev:wpcom-block-editor-nux": "check-npm-client && npm run wpcom-block-editor-nux",
		"build:wpcom-block-editor-nux": "check-npm-client && NODE_ENV=production npm run wpcom-block-editor-nux",
		"dev": "check-npm-client && node bin/npm-run-build.js --dev",
		"build": "check-npm-client && node bin/npm-run-build.js --build",
		"test:js": "check-npm-client && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --colors",
		"test:js:help": "check-npm-client && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --help --colors",
		"test:js:watch": "check-npm-client && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --watch --colors",
		"test:js:update-snapshots": "check-npm-client && npx wp-scripts test-unit-js -u --config='bin/js-unit-config.js' --colors",
		"test:e2e": "check-npm-client && npx wp-scripts test-e2e --wordpress-base-url='http://localhost:4013' --colors",
		"test:e2e:watch": "check-npm-client && npm run test-e2e -- --watch",
		"clean": "check-npm-client && npx rimraf dist full-site-editing-plugin/*/dist",
		"prebuild": "check-npm-client && npm run clean",
		"predev": "check-npm-client && npm run clean",
		"sync:blog-posts-block": "check-npm-client && ./bin/sync-blog-posts-block.sh",
		"wpcom-sync": "check-npm-client && ./bin/wpcom-watch-and-sync.sh",
		"prepare": "check-npm-client && npm run sync:blog-posts-block --nodemodules"
	},
	"dependencies": {
		"@wordpress/api-fetch": "*",
		"@wordpress/block-editor": "*",
		"@wordpress/blocks": "*",
		"@wordpress/components": "*",
		"@wordpress/compose": "*",
		"@wordpress/data": "*",
		"@wordpress/date": "*",
		"@wordpress/dom-ready": "*",
		"@wordpress/edit-post": "*",
		"@wordpress/editor": "*",
		"@wordpress/element": "*",
		"@wordpress/hooks": "*",
		"@wordpress/html-entities": "*",
		"@wordpress/i18n": "*",
		"@wordpress/keycodes": "*",
		"@wordpress/plugins": "*",
		"@wordpress/url": "*",
		"classnames": "2.2.6",
		"lodash": "*",
		"moment": "*",
		"newspack-blocks": "github:Automattic/newspack-blocks#v1.2.1"
	}
}
