{
	"name": "@automattic/full-site-editing",
	"version": "1.1.0",
	"description": "Plugin for full site editing with the block editor.",
	"sideEffects": true,
	"repository": {
		"type": "git",
		"url": "git://github.com/Automattic/wp-calypso.git",
		"directory": "apps/full-site-editing"
	},
	"private": true,
	"author": "Automattic Inc.",
	"license": "GPL-2.0-or-later",
	"bugs": {
		"url": "https://github.com/Automattic/wp-calypso/issues"
	},
	"homepage": "https://github.com/Automattic/wp-calypso",
	"scripts": {
		"full-site-editing": "node ../../bin/check-package-manager.js && calypso-build --source='dotcom-fse'",
		"dev:full-site-editing": "node ../../bin/check-package-manager.js && yarn run full-site-editing",
		"build:full-site-editing": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run full-site-editing",
		"posts-list-block": "node ../../bin/check-package-manager.js && calypso-build --source='posts-list-block'",
		"dev:posts-list-block": "node ../../bin/check-package-manager.js && yarn run posts-list-block",
		"build:posts-list-block": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run posts-list-block",
		"common": "node ../../bin/check-package-manager.js && calypso-build --source='common'",
		"dev:common": "node ../../bin/check-package-manager.js && yarn run common",
		"build:common": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run common",
		"blog-posts-block-editor": "node ../../bin/check-package-manager.js && calypso-build --source='blog-posts-block/editor'",
		"blog-posts-block-view": "node ../../bin/check-package-manager.js && calypso-build --source='blog-posts-block/view'",
		"blog-posts-block": "node ../../bin/check-package-manager.js && npm-run-all --parallel blog-posts-block-*",
		"dev:blog-posts-block": "node ../../bin/check-package-manager.js && yarn run blog-posts-block",
		"build:blog-posts-block": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run blog-posts-block",
		"starter-page-templates": "node ../../bin/check-package-manager.js && calypso-build --source='starter-page-templates'",
		"dev:starter-page-templates": "node ../../bin/check-package-manager.js && yarn run starter-page-templates",
		"build:starter-page-templates": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run starter-page-templates",
		"event-countdown-block": "node ../../bin/check-package-manager.js && calypso-build --source='event-countdown-block'",
		"dev:event-countdown-block": "node ../../bin/check-package-manager.js && yarn run event-countdown-block",
		"build:event-countdown-block": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run event-countdown-block",
		"global-styles": "node ../../bin/check-package-manager.js && calypso-build --source='global-styles'",
		"build:global-styles": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run global-styles",
		"dev:global-styles": "node ../../bin/check-package-manager.js && yarn run global-styles",
		"jetpack-timeline": "node ../../bin/check-package-manager.js && calypso-build --source='jetpack-timeline'",
		"dev:jetpack-timeline": "node ../../bin/check-package-manager.js && yarn run jetpack-timeline",
		"build:jetpack-timeline": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run jetpack-timeline",
		"wpcom-block-editor-nux": "node ../../bin/check-package-manager.js && calypso-build --source='wpcom-block-editor-nux'",
		"dev:wpcom-block-editor-nux": "node ../../bin/check-package-manager.js && yarn run wpcom-block-editor-nux",
		"build:wpcom-block-editor-nux": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run wpcom-block-editor-nux",
		"dev": "node ../../bin/check-package-manager.js && node bin/npm-run-build.js --dev",
		"build": "node ../../bin/check-package-manager.js && node bin/npm-run-build.js --build",
		"test:js": "node ../../bin/check-package-manager.js && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --colors",
		"test:js:help": "node ../../bin/check-package-manager.js && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --help --colors",
		"test:js:watch": "node ../../bin/check-package-manager.js && npx wp-scripts test-unit-js --config='bin/js-unit-config.js' --watch --colors",
		"test:js:update-snapshots": "node ../../bin/check-package-manager.js && npx wp-scripts test-unit-js -u --config='bin/js-unit-config.js' --colors",
		"test:e2e": "node ../../bin/check-package-manager.js && npx wp-scripts test-e2e --wordpress-base-url='http://localhost:4013' --colors",
		"test:e2e:watch": "node ../../bin/check-package-manager.js && npm run test-e2e -- --watch",
		"clean": "node ../../bin/check-package-manager.js && npx rimraf dist full-site-editing-plugin/*/dist",
		"prebuild": "node ../../bin/check-package-manager.js && yarn run clean",
		"predev": "node ../../bin/check-package-manager.js && yarn run clean",
		"sync:blog-posts-block": "node ../../bin/check-package-manager.js && ./bin/sync-blog-posts-block.sh",
		"wpcom-sync": "node ../../bin/check-package-manager.js && ./bin/wpcom-watch-and-sync.sh",
		"prepare": "node ../../bin/check-package-manager.js && npm run sync:blog-posts-block --nodemodules"
	},
	"dependencies": {
		"@wordpress/api-fetch": "*",
		"@wordpress/block-editor": "*",
		"@wordpress/blocks": "*",
		"@wordpress/components": "*",
		"@wordpress/compose": "*",
		"@wordpress/data": "*",
		"@wordpress/date": "*",
		"@wordpress/dom-ready": "*",
		"@wordpress/edit-post": "*",
		"@wordpress/editor": "*",
		"@wordpress/element": "*",
		"@wordpress/hooks": "*",
		"@wordpress/html-entities": "*",
		"@wordpress/i18n": "*",
		"@wordpress/keycodes": "*",
		"@wordpress/plugins": "*",
		"@wordpress/url": "*",
		"classnames": "2.2.6",
		"lodash": "*",
		"moment": "*",
		"newspack-blocks": "github:Automattic/newspack-blocks#v1.2.1"
	}
}
