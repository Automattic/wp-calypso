{
	"name": "@automattic/wpcom-block-editor",
	"version": "1.0.0-alpha.0",
	"description": "Utilities for the block editor in WordPress.com integration",
	"sideEffects": true,
	"repository": {
		"type": "git",
		"url": "git://github.com/Automattic/wp-calypso.git",
		"directory": "apps/wpcom-block-editor"
	},
	"private": true,
	"author": "Automattic Inc.",
	"license": "GPL-2.0-or-later",
	"bugs": {
		"url": "https://github.com/Automattic/wp-calypso/issues"
	},
	"homepage": "https://github.com/Automattic/wp-calypso",
	"scripts": {
		"bundle": "node ../../bin/check-package-manager.js && calypso-build --env.WP",
		"build:dev": "node ../../bin/check-package-manager.js && npm run bundle --",
		"build:prod": "node ../../bin/check-package-manager.js && NODE_ENV=production yarn run bundle --",
		"build": "node ../../bin/check-package-manager.js && npm-run-all --parallel \"build:* -- {@}\" --",
		"clean": "node ../../bin/check-package-manager.js && npx rimraf dist",
		"prebuild": "node ../../bin/check-package-manager.js && yarn run clean"
	},
	"dependencies": {
		"debug": "4.1.1"
	}
}
