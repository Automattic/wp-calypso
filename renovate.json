{
  "extends": [
    "config:base",
    "default:pinDigestsDisabled"
  ],
  "statusCheckVerify": true,
  "ignoreDeps": [
    "jquery"
  ],
  "packageRules": [
    {
      "extends": "monorepo:babel",
      "groupName": "babel"
    },
    {
      "extends": "monorepo:jest",
      "groupName": "jest"
    },
    {
      "extends": "packages:eslint",
      "groupName": "eslint"
    },
    {
      "extends": "monorepo:lodash",
      "groupName": "lodash"
    },
    {
      "extends": "monorepo:react",
      "groupName": "react"
    }
  ],
  "labels": [
    "Framework",
    "[Type] Task",
    "[Status] Needs Review"
  ]
}
