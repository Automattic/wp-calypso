{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": [
						"last 2 versions"
					]
				}
			}
		],
		"stage-2"
	],
	"plugins": [
		"add-module-exports",
		"syntax-jsx",
		"transform-export-extensions",
		"transform-react-display-name",
		"transform-react-jsx",
		"transform-runtime",
		[
			"transform-imports",
			{
				"state/selectors": {
					"transform": "state/selectors/${member}",
					"kebabCase": true
				}
			}
		]
	],
	"env": {
		"test": {
			"plugins": [
				"./server/bundler/babel/babel-lodash-es",
				[
					"transform-builtin-extend",
					{
						"globals": [
							"Error"
						]
					}
				]
			]
		}
	}
}
