{
	"type": "object",
	"properties": {
		"extra": {
			"type": "object",
			"properties": {
				"sirenSiret": {
					"type": "string",
					"pattern": "^[0-9]*$",
					"anyOf": [
						{ "minLength": 9, "maxLength": 9 },
						{ "minLength": 14, "maxLength": 14 }
					]
				},
				"registrantType": {
					"enum": [ "individual", "organization" ]
				},
				"registrantVatId": {
					"type": "string",
					"anyOf": [
						{ "pattern": "^[a-zA-Z]{2}.{2}[0-9]{2,10}[a-zA-Z0-9]{4}$" },
						{ "pattern": "^.*GB.*$" }
					]
				},
				"trademarkNumber": {
					"minLength": 9,
					"maxLength": 9,
					"pattern": "^[0-9]*$"
				}
			}
		}
	},
	"note": "wpcom.undocumented already has `mapKeysRecursively()` & `camelCase()`"
}
