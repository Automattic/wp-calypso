				"product_name": { "type": "string" },
				"product_slug": { "type": "string" },
				"meta": { "type": "string" },
				"cost": { "type": "number" },
				"orig_cost": { "type": [ "number", "null" ] },
				"currency": { "type": "string" },
				"volume": { "type": "number" },
				"quantity": { "type": [ "number", "null" ] },
				"free_trial": { "type": "boolean" },
				"is_domain_registration": { "type": "boolean" }
