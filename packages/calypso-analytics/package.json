{
	"name": "@automattic/calypso-analytics",
	"version": "1.0.0-alpha.1",
	"description": "Automattic Analytics",
	"homepage": "https://github.com/Automattic/wp-calypso",
	"license": "GPL-2.0-or-later",
	"author": "Automattic Inc.",
	"sideEffects": true,
	"main": "dist/cjs/index",
	"module": "dist/esm/index",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Automattic/wp-calypso.git",
		"directory": "packages/calypso-analytics"
	},
	"files": [
		"dist",
		"src"
	],
	"types": "dist/types",
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/Automattic/wp-calypso/issues"
	},
	"scripts": {
		"clean": "node ../../bin/check-package-manager.js && npx rimraf dist \"../../.tsc-cache/packages__calypso-analytics*\"",
		"prepublish": "node ../../bin/check-package-manager.js && yarn run clean",
		"prepare": "node ../../bin/check-package-manager.js && tsc --project ./tsconfig.json && tsc --project ./tsconfig-cjs.json",
		"watch": "node ../../bin/check-package-manager.js && tsc --project ./tsconfig.json --watch"
	},
	"dependencies": {
		"@automattic/load-script": "^1.0.0",
		"cookie": "0.4.0",
		"debug": "^4.1.1",
		"hash.js": "^1.1.7",
		"lodash": "^4.17.15",
		"tslib": "^1.10.0"
	}
}
