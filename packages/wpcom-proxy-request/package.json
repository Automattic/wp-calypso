{
	"name": "wpcom-proxy-request",
	"version": "6.0.0",
	"description": "Proxied cookie-authenticated REST API requests to WordPress.com",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"calypso:src": "src/index.ts",
	"types": "dist/types",
	"sideEffects": false,
	"keywords": [
		"browser",
		"wpcom",
		"wordpress",
		"rest",
		"api",
		"cookie"
	],
	"author": "Automattic, Inc.",
	"license": "MIT",
	"homepage": "https://github.com/Automattic/wp-calypso",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Automattic/wp-calypso.git",
		"directory": "packages/wpcom-proxy-request"
	},
	"private": true,
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/Automattic/wp-calypso/issues"
	},
	"files": [
		"dist",
		"History.md",
		"README.md"
	],
	"scripts": {
		"clean": "tsc --build ./tsconfig.json ./tsconfig-cjs.json --clean",
		"build": "tsc --build ./tsconfig.json ./tsconfig-cjs.json",
		"prepack": "yarn run clean && yarn run build",
		"watch": "tsc --build ./tsconfig.json --watch"
	},
	"dependencies": {
		"@babel/runtime": "^7.12.5",
		"debug": "^4.1.1",
		"progress-event": "^1.0.0",
		"tslib": "^1.10.0",
		"uuid": "^7.0.1",
		"wp-error": "^1.3.0"
	},
	"devDependencies": {
		"chai": "^4.2.0"
