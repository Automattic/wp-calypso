		"@automattic/calypso-typescript-config": "workspace:^",
		"@storybook/addon-backgrounds": "^7.4.6",
		"@storybook/cli": "^7.4.6",
		"@storybook/react": "^7.4.6",
		"typescript": "^5.2.2"
	},
	"peerDependencies": {
		"@wordpress/data": "^9.11.0",
